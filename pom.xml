<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>io.enoy</groupId>
  <artifactId>spring-tg-security</artifactId>
  <version>1.1.1</version>
  <packaging>jar</packaging>
  
  <name>Spring-tg-security</name>
  <url>http://maven.apache.org</url>
  <properties>
		<maven.compiler.source>1.8</maven.compiler.source>
		<maven.compiler.target>1.8</maven.compiler.target>
		<telegrambots.version>4.1.2</telegrambots.version>
		<spring-boot-dependencies.version>1.5.7.RELEASE</spring-boot-dependencies.version>
	</properties>
	
	<repositories>
      <repository>
		<id>jcenter</id>
		<url>http://jcenter.bintray.com</url>
	  </repository>
	  <repository>
		<id>jitpack.io</id>
		<url>https://jitpack.io</url>
	  </repository>
    </repositories>
	
	
	<dependencies>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter</artifactId>
			<version>${spring-boot-dependencies.version}</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.projectlombok</groupId>
			<artifactId>lombok</artifactId>
			<version>1.16.12</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-security</artifactId>
			<version>${spring-boot-dependencies.version}</version>
			<scope>provided</scope>
		</dependency>
		
		<dependency>
			<groupId>org.telegram</groupId>
			<artifactId>telegrambots</artifactId>
			<version>${telegrambots.version}</version>
		</dependency>

		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-aop</artifactId>
			<version>${spring-boot-dependencies.version}</version>
			<scope>provided</scope>
		</dependency>

		<dependency>
    		<groupId>com.github.LeandroErllet</groupId>
    		<artifactId>spring-tg-core</artifactId>
    		<version>master</version>
        </dependency>

	</dependencies>

</project>